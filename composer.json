{
    "name": "propaganistas/laravel-translatable-bootforms",
    "description": "Empowers adamwathan/bootforms with dimsav/laravel-translatable.",
    "keywords": ["laravel", "bootforms", "translatable", "i18n", "form"],
    "license": "MIT",
    "authors": [
        {
            "name": "Propaganistas",
            "email": "Propaganistas@users.noreply.github.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.0",
        "adamwathan/bootforms": "^0.8",
        "dimsav/laravel-translatable": "^5.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Propaganistas\\LaravelTranslatableBootForms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Propaganistas\\LaravelTranslatableBootForms\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
