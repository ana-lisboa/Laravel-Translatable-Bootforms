{
  "name": "ana-lisboa/laravel-translatable-bootforms",
  "description": "Fork from Propagandistas/laravel-translatable-bootforms. Empowers adamwathan/bootforms with dimsav/laravel-translatable.",
  "keywords": [
    "laravel",
    "bootforms",
    "translatable",
    "i18n",
    "form"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ana Lisboa",
      "email": "ana@analisboa.pt"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ana-lisboa/laravel-translatable-bootforms"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.7.*",
    "illuminate/support": ">=5.0,<5.6",
    "adamwathan/form": ">=0.8.0 <0.8.13 || ^0.9",
    "adamwathan/bootforms": "^0.8 || ^0.9",
    "dimsav/laravel-translatable": ">=5.0"
  },
  "require-dev": {
    "laravel/framework": "5.7.*",
    "orchestra/testbench": "^3.0",
    "phpunit/phpunit": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "AnaLisboa\\LaravelTranslatableBootForms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AnaLisboa\\LaravelTranslatableBootForms\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true
}
